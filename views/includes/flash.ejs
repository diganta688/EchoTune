<%if(success && success.length){ %>
    <div class="success" style="width: 100%; display: flex; justify-content: center; ">
        <div class="alert alert-success alert-dismissible fade show col-6 " role="alert">
            <i class="fa-solid fa-circle-check" style="margin-right: 1rem;"></i>
            <%= success%>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
    </div>
<%}%>
<%if(error && error.length){ %>
    <div class="error" style="width: 100%; display: flex; justify-content: center;">
        <div class="alert alert-danger alert-dismissible fade show col-6 " role="alert">
            <i class="fa-solid fa-triangle-exclamation " style="margin-right: 1rem;"></i>
            <%= error%>
            <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
    </div>
<%}%>